<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Address</th>
      <th scope="col">Phone</th>
      <th scope="col">Image</th>
      <th scope="col">Edit</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <th scope="row"><%= user.id %></th>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.address %></td>
          <td><%= user.phone %></td>
        <% if user.image.present? %>
          <td><%= image_tag user.image, style: 'width: 200px; display: block' %></td>
        <% else %>
          <td>None</td>
        <% end %>
        <td><%= link_to 'Edit Profile' , edit_user_registration_path(user.id), :class => "btn btn-primary" %></td>
      </tr>
    <% end %>
  </tbody>
</table>